<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <Match>
    <!-- Exclude files generated by the javacc-maven-plugin -->
    <Or>
      <Source name="~.*/javacc/LabelParser\.java*"/>
      <Source name="~.*/javacc/LabelParserTokenManager\.java*"/>
      <Source name="~.*/javacc/SimpleCharStream\.java*"/>
      <Source name="~.*/javacc/TokenMgrError\.java*"/>
    </Or>
  </Match>
</FindBugsFilter>
